{
  "canisters": {
    "child": {
      "build": [
        "cargo build --target wasm32-unknown-unknown --package  child --release",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/child.wasm -o target/wasm32-unknown-unknown/release/child_opt.wasm"
      ],
      "candid": "src/_child/backend/child.did",
      "wasm": "target/wasm32-unknown-unknown/release/child_opt.wasm",
      "type": "custom"
    },
    "parent": {
      "package": "parent",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package  parent --release",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/parent.wasm -o target/wasm32-unknown-unknown/release/parent_opt.wasm"
      ],
      "candid": "src/_parent/backend/parent.did",
      "wasm": "target/wasm32-unknown-unknown/release/parent_opt.wasm",
      "type": "custom",
      "dependencies": []
    },
    "ledger": {
      "type": "custom",
      "candid": "src/_meta/ledger/ledger.public.did",
      "wasm": "src/_meta/ledger/ledger.wasm"
    },
    "cmc": {
      "type": "custom",
      "candid": "src/_meta/minting/cmc.did",
      "wasm": "src/_meta/minting/cmc.wasm"
    },
    "utils": {
      "main": "src/_parent/utils/main.mo",
      "type": "motoko"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.10.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
